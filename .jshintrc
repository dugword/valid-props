{
    "node": true,
    "esnext": true,
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "futurehostile": true,
    "latedef": "nofunc",
    // "maxcomplexity": 2,
    "maxdepth": 4,
    "noarg": true,
    "nocomma": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": true,
    "singleGroups": true,
    "strict": "global",
    "regexp": true,
    "undef": true,
    "unused": true,
    "varstmt": true,
    "predef": [
        "console",
        "module",
        "angular",
        "require",
        "it",
        "before",
        "after",
        "describe",
        "$",
        "document",
        "AQL"
    ]
}
